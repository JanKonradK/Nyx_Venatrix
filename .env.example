# Environment Variables
GROK_API_KEY=your_grok_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
AGENT_MODEL=grok-4-1-fast-reasoning
EMBEDDING_MODEL=text-embedding-3-small

# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=nyx_venatrix
DATABASE_URL=postgres://postgres:postgres@postgres:5432/nyx_venatrix

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379

# Agent Configuration
AGENT_URL=http://agent:8000
QDRANT_URI=http://qdrant:6333

# Telegram Bot (Optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_chat_id

# CAPTCHA Solving (2captcha.com)
TWOCAPTCHA_API_KEY=your_2captcha_api_key_here

# Frontend
VITE_API_URL=http://localhost:3000

# Effort Modes (LOW, MEDIUM, HIGH)
DEFAULT_EFFORT_MODE=MEDIUM
MAX_TOKENS_PER_QUESTION=512
MAX_TOKENS_PER_APP=7000

# KDB Salary Oracle (Optional)
KDB_LICENSE_B64=your_kdb_license_base64
KDB_HOST=kdb
KDB_PORT=5000
